@{
    ViewData["Title"] = "Suspender";
}

<div class="card shadow">
    <div class="card-header text-center">
        <h4>Ordem de Fornecimento</h4>
    </div>
    <div class="card-body">
        <div class="text-center">
            <span class="text-muted">Selecione uma Unidade Administrativa e um ano</span>
        </div>
        <div class="d-flex justify-content-center text-black-50 mt-2">
            <div class="col-3">
                <select id="UnidadeAdministrativaa" class="custom-select OrderSelect" asp-items="(SelectList)ViewBag.ListUnidadeAdministrativa">
                    <option value="">...</option>
                </select>
            </div>
            <div class="col-3">
                <select id="AnoAta" class="custom-select OrderSelect" asp-items="new SelectList(ViewBag.ListYears)">
                    <option value="">Selecione um Ano</option>
                </select>
            </div>
        </div>
        <hr />
        <div id="result"></div>
    </div>
</div>
@section Scripts {
    <script src="~/js/Programacao/ProgramacaoAjax.js" type="module"></script>
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

    }
